apiVersion: networking.k8s.io/v1beta1 
kind: Ingress
metadata:
  name: example-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /$1
spec:
  rules:
  - host: final.lo
    http:
      paths:
      - path: /
        backend:
          serviceName: final
          servicePort: 8080